{
    "env": {
        "es2021": true
    },
    "extends": [
        "airbnb",
        "plugin:react/recommended",
        "eslint:recommended",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/function-component-definition": "off",
        "no-nested-ternary": "off",
        "no-undef": "off",
        "react/prop-types": "off",
        // "no-loop-func": "off",
        "no-unused-expressions": "off"
        // "no-plusplus": "off"
    }
}
