{
  "name": "moviesshowcase",
  "version": "1.0.0",
  "description": "Movies Showcase isomorphic web app made with React",
  "license": "MIT",
  "author": "Audrey Kadjar",
  "scripts": {
    "lint": "eslint ./src --fix",
    "style-lint": "&& npx stylelint '**/*.scss' --fix",
    "dev": "NODE_ENV=development nodemon src/api/index.js && nodemon src/serverRendering/index.js",
    "build": "webpack --mode=production",
    "unit-test": "jest --testPathPattern=src/client/unit-testing --watchAll"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^.+\\.svg$": "jest-svg-transformer",
      "^.+\\.(scss)$": "babel-jest"
    }
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "nodemon": "^2.0.20",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-scss": "^4.3.0"
  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "axios": "^1.2.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "cors": "^2.8.5",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.0",
    "intersection-observer": "^0.12.2",
    "jest-svg-transformer": "^1.0.0",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^8.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.1",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-hot-server-middleware": "^0.6.1"
  }
}
